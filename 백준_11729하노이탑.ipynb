        "def hanoi(n,a,b):\n",
        "    if n == 1:\n",
        "        print(a, b)\n",
        "        return\n",
        "    \n",
        "    hanoi(n-1,a,6-a-b) # 시작막대에 남은 고리를 나머지 막대에 옮겨줌\n",
        "    print(a, b)\n",
        "    hanoi(n-1,6-a-b,b)\n",
        "\n",
        "N = int(input())\n",
        "print(2**N-1)\n",
        "hanoi(N,1,3)"
      ],
